
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor _httpContextAccessor

@{
    string roles;
    var usernames = _httpContextAccessor.HttpContext.Session.GetString("UserName").Trim();
    var Role = _httpContextAccessor.HttpContext.Session.GetString("Role").Trim();
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MetLifeInsurance</title>

   @* <link href="cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">*@
    <link href="cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">

    <link href="~/Content/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <link href="~/Content/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">

    <link href="~/Content/vendors/nprogress/nprogress.css" rel="stylesheet">

    <link href="~/Content/vendors/iCheck/skins/flat/green.css" rel="stylesheet">

    <link href="~/Content/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">

    <link href="~/Content/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
   
    <link href="~/Content/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet" />


    <link href="~/Content/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/vendors/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/vendors/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/vendors/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">

    <link href="~/Content/build/css/custom.min.css" rel="stylesheet">


    <style>
        .disabled-link {
            pointer-events: none;
            color: gray;
            text-decoration: none;
        }
    </style>

</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;float:right;">
                        <a href="/home" class="site_title" style="padding-left:30px">
                            <img src="~/Content/build/images/MetLifeCrop.png" style="40px;height:40px;object-fit:contain" />
                            <span>Met Life</span>
                        </a>

                    </div>

                    <div class="clearfix"></div>

                    <!-- menu profile quick info -->
                    @*<div class="profile clearfix">
                    <div class="profile_pic">
                    <img src="../wwwroot/" alt="..." class="img-circle profile_img">
                    </div>
                    <div class="profile_info">
                    <span>Welcome,</span>
                    <h2>John Doe</h2>
                    </div>
                    </div>*@
                    <!-- /menu profile quick info -->
                    <br />

                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section active">
                            <ul class="nav side-menu">
                                <li class="active">
                                    <a class="disabled-link"><i class="fa fa-home"></i> Home <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: block;">
                                        <li class="current-page"><a href="/Dashboard">Home Testing Service Dashboard</a></li>
                                        <li><a href="/doctor/dashboard">Physical Doctor Dashboard</a></li>
                                        <li><a href="/tele/doctor/dashboard">Tele Doctor Dashboard</a></li>
                                        <li><a href="/virtual/doctor/dashboard">Virtual Doctor Dashboard</a></li>
                                        <li><a href="/international/doctor/dashboard">International Doctor Dashboard</a></li>
                                      
                                    </ul>
                                </li>
                                @if (Role == "admin")
                                {
                                    <li class="active">
                                        <a class="disabled-link"><i class="fa fa-windows"></i> Setting <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu" style="display: block;">
                                            <li><a href="/UsersList">User Management</a></li>
                                        </ul>
                                    </li>
                                }

                                <li class="active">
                                    <a class="disabled-link"><i class="fa fa-book"></i> Booking List <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: block;">
                                        <li><a href="/hometesting">Home Testing</a></li>
                                        <li><a href="/physical/doctor/list">Physical Doctor</a></li>
                                        <li><a href="/tele/doctor/list">Tele Doctor</a></li>
                                        <li><a href="/virtual/doctor/list">Virtual Doctor</a></li>
                                        <li><a href="/international/doctor/list">International Doctor</a></li>
                                        <li><a href="/order/list">Medicine Order</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>

                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                    </div>
                    <nav class="nav navbar-nav">
                        <ul class=" navbar-right">

                            <li class="nav-item dropdown open" style="padding-left: 15px;">

                                @if (usernames != null)
                                {
                                    var username = usernames;

                                    <a class="user-profile dropdown-toggle" aria-haspopup="true" id="navbarDropdown"
                                       data-toggle="dropdown" aria-expanded="false">
                                        @username
                                    </a>
                                    <div class="dropdown-menu dropdown-usermenu pull-right" aria-labelledby="navbarDropdown">
                                        <a href="/Account/LogOut" class="btn btn-link navbar-btn navbar-link" style="color:black;font-size:12px;text-decoration:none;width: 200px;">Logout</a>

                                    </div>
                                    
                                }
                                else
                                {
                                <li class="nav-item">
                                    <a class="nav-link text-dark" id="login" href="/Account/Login">Login</a>
                                </li>

                                }
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
        <div class="right_col" role="main">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>

            <!-- footer content -->
            <!-- /footer content -->

        </div>
    </div>


    <script src="~/Content/vendors/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/vendors/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/vendors/fastclick/lib/fastclick.js"></script>
    <script src="~/Content/vendors/Chart.js/dist/Chart.min.js"></script>
    <script src="~/Content/vendors/gauge.js/dist/gauge.min.js"></script>
    <script src="~/Content/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/Content/vendors/iCheck/icheck.min.js"></script>
    <script src="~/Content/vendors/skycons/skycons.js"></script>
    <script src="~/Content/vendors/Flot/jquery.flot.js"></script>
    <script src="~/Content/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/Content/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/Content/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/Content/vendors/Flot/jquery.flot.resize.js"></script>
    <script src="~/Content/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="~/Content/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/Content/vendors/flot.curvedlines/curvedLines.js"></script>
    <script src="~/Content/vendors/DateJS/build/date.js"></script>
    <script src="~/Content/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="~/Content/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="~/Content/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <script src="~/Content/vendors/moment/min/moment.min.js"></script>
    <script src="~/Content/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="~/Content/vendors/jszip/dist/jszip.min.js"></script>
    <script src="~/Content/vendors/pdfmake/build/pdfmake.min.js"></script>
    <script src="~/Content/vendors/pdfmake/build/vfs_fonts.js"></script>
    <script src="~/Content/build/js/custom.min.js"></script>
    <script defer src="https://static.cloudflareinsights.com/beacon.min.js/v52afc6f149f6479b8c77fa569edb01181681764108816" integrity="sha512-jGCTpDpBAYDGNYR5ztKt4BQPGef1P0giN6ZGVUi835kFF88FOmmn8jBQWNgrNd8g/Yu421NdgWhwQoaOPFflDw==" data-cf-beacon='{"rayId":"7c38c4c19d199e80","token":"cd0b4b3a733644fc843ef0b185f98241","version":"2023.4.0","si":100}' crossorigin="anonymous"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
