<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>
    <!-- Bootstrap -->
    @Styles.Render("~/Content/Gentella/bootstrap/dist/css/bootstrap.min.css")
    <!-- Font Awesome -->
    @Styles.Render("~/Content/Gentella/font-awesome/css/font-awesome.min.css")
    <!-- NProgress -->
    @Styles.Render("~/Content/Gentella/nprogress/nprogress.css")

    <!-- Animate.css -->
    @Styles.Render("~/Content/Gentella/animate.css/animate.min.css")

    <!-- bootstrap-datetimepicker -->
    @Styles.Render("~/Content/Gentella/bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.css")

    <!-- iCheck -->
    @Styles.Render("~/Content/Gentella/iCheck/skins/flat/green.css")

    <!-- bootstrap-progressbar -->
    @Styles.Render("~/Content/Gentella/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css")
    <!-- JQVMap -->
    @Styles.Render("~/Content/Gentella/jqvmap/dist/jqvmap.min.css")
    <!-- bootstrap-daterangepicker -->
    @Styles.Render("~/Content/Gentella/bootstrap-daterangepicker/daterangepicker.css")

    <!-- Datatables -->
    @Styles.Render("~/Content/Gentella/datatables.net-bs/css/dataTables.bootstrap.min.css")
    @Styles.Render("~/Content/Gentella/datatables.net-buttons-bs/css/buttons.bootstrap.min.css")
    @Styles.Render("~/Content/Gentella/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css")
    @Styles.Render("~/Content/Gentella/datatables.net-responsive-bs/css/responsive.bootstrap.min.css")
    @Styles.Render("~/Content/Gentella/datatables.net-scroller-bs/css/scroller.bootstrap.min.css")

    <!--Bootstrap File Upload-->
    @Styles.Render("~/Content/Gentella/FileUpload/bootstrap-fileupload.min.css")

    <!-- Custom Theme Style -->
    @Styles.Render("~/Content/Gentella/css/custom.min.css")
    <!-- Full Calendar Style -->
    @Styles.Render("~/Content/Gentella/fullcalendar/dist/fullcalendar.css")
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="clearfix"></div>
                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_info">
                            <h4>@PayrollManagementSystem.Classes.Utilities.LoggedInEmployee.Company</h4>
                            <span>Welcome,</span><h2>@WebSecurity.CurrentUserName</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        @{
                            Html.RenderAction("SideMenu", "Home");
                        }
                    </div>
                </div>
                <!-- /sidebar menu -->
            </div>
        </div>
        <!-- top navigation -->
        <div class="top_nav">
            <div class="nav_menu">
                <nav>
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                    </div>
                    <ul class="nav navbar-nav navbar-right">
                        <li class="">
                            <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                @WebSecurity.CurrentUserName
                                <span class=" fa fa-angle-down"></span>
                            </a>
                            <ul class="dropdown-menu dropdown-usermenu pull-right">
                                <li><a href="@Url.Action("ChangePassword", "Account")"><i class="fa fa-key pull-right"></i>Change Password </a></li>
                                <li><a href="@Url.Action("SignOut", "Account")"><i class="fa fa-sign-out pull-right"></i>Log Out</a></li>
                            </ul>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
        <!-- /top navigation -->
        <!-- page content -->
        <div class="right_col" role="main">
            <div class="">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /page content -->
        <!-- footer content -->
        <footer>
            <div class="pull-right">
                Payroll Management System Powered by <a href="http://www.teamnext.com.np">Team Next I.C.T. Solutions Pvt. Ltd.</a>
            </div>
            <div class="clearfix"></div>
        </footer>
        <!-- /footer content -->
    </div>
    </div>
    <!-- jQuery -->
    @Scripts.Render("~/Content/Gentella/jquery/dist/jquery.min.js")
    <!-- Bootstrap -->
    @Scripts.Render("~/Content/Gentella/bootstrap/dist/js/bootstrap.min.js")
    <!-- FastClick -->
    @Scripts.Render("~/Content/Gentella/fastclick/lib/fastclick.js")
    <!-- NProgress -->
    @Scripts.Render("~/Content/Gentella/nprogress/nprogress.js")
    <!-- Chart.js -->
    @Scripts.Render("~/Content/Gentella/Chart.js/dist/Chart.min.js")
    <!-- gauge.js -->
    @Scripts.Render("~/Content/Gentella/gauge.js/dist/gauge.min.js")
    <!-- bootstrap-progressbar -->
    @Scripts.Render("~/Content/Gentella/bootstrap-progressbar/bootstrap-progressbar.min.js")
    <!-- iCheck -->
    @Scripts.Render("~/Content/Gentella/iCheck/icheck.min.js")
    <!-- Skycons -->
    @Scripts.Render("~/Content/Gentella/skycons/skycons.js")
    <!-- Flot -->
    @Scripts.Render("~/Content/Gentella/Flot/jquery.flot.js")
    @Scripts.Render("~/Content/Gentella/Flot/jquery.flot.pie.js")
    @Scripts.Render("~/Content/Gentella/Flot/jquery.flot.time.js")
    @Scripts.Render("~/Content/Gentella/Flot/jquery.flot.stack.js")
    @Scripts.Render("~/Content/Gentella/Flot/jquery.flot.resize.js")

    <!-- Flot plugins -->
    @Scripts.Render("~/Content/Gentella/flot.orderbars/js/jquery.flot.orderBars.js")
    @Scripts.Render("~/Content/Gentella/flot-spline/js/jquery.flot.spline.min.js")
    @Scripts.Render("~/Content/Gentella/flot.curvedlines/curvedLines.js")
    <!-- DateJS -->
    @Scripts.Render("~/Content/Gentella/DateJS/build/date.js")
    <!-- JQVMap -->
    @Scripts.Render("~/Content/Gentella/jqvmap/dist/jquery.vmap.js")
    @Scripts.Render("~/Content/Gentella/jqvmap/dist/maps/jquery.vmap.world.js")
    @Scripts.Render("~/Content/Gentella/jqvmap/examples/js/jquery.vmap.sampledata.js")
    <!-- bootstrap-daterangepicker -->
    @Scripts.Render("~/Content/Gentella/moment/min/moment.min.js")
    <!-- bootstrap-datetimepicker -->
    @Scripts.Render("~/Content/Gentella/bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js")
    @Scripts.Render("~/Content/Gentella/bootstrap-daterangepicker/daterangepicker.js")


    <!-- Datatables -->
    @Scripts.Render("~/Content/Gentella/datatables.net/js/jquery.dataTables.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-bs/js/dataTables.bootstrap.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-buttons/js/dataTables.buttons.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-buttons-bs/js/buttons.bootstrap.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-buttons/js/buttons.flash.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-buttons/js/buttons.html5.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-buttons/js/buttons.print.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-keytable/js/dataTables.keyTable.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-responsive/js/dataTables.responsive.min.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-responsive-bs/js/responsive.bootstrap.js")
    @Scripts.Render("~/Content/Gentella/datatables.net-scroller/js/dataTables.scroller.min.js")
    @Scripts.Render("~/Content/Gentella/FileUpload/bootstrap-fileupload.js")

    @Scripts.Render("~/Content/Gentella/jszip/dist/jszip.min.js")
    @Scripts.Render("~/Content/Gentella/pdfmake/build/pdfmake.min.js")
    @Scripts.Render("~/Content/Gentella/pdfmake/build/vfs_fonts.js")
    <!-- Custom Theme Scripts -->
    @Scripts.Render("~/Content/Gentella/js/custom.js")
    <!-- Full Calendar Theme Scripts -->
    @Scripts.Render("~/Content/Gentella/fullcalendar/dist/fullcalendar.js")


    <script type='text/javascript'>

        $('.dtp').datetimepicker({
            ignoreReadonly: true,
            allowInputToggle: true
        });

        $('.tp').datetimepicker({
            ignoreReadonly: true,
            allowInputToggle: true,
            format: 'HH:mm A'
        });

    </script>

</body>
</html>
